<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:type="MailApp">
    <Id>123e4567-e89b-12d3-a456-426614174000</Id>
    <Version>1.0.0.0</Version>
    <ProviderName>Your Name</ProviderName>
    <DefaultLocale>en-US</DefaultLocale>

    <DisplayName DefaultValue="Email Validator">
        <Override Locale="en-US" Value="Email Validator"/>
    </DisplayName>

    <Description DefaultValue="Checks email domains for validity">
        <Override Locale="en-US" Value="Checks email domains for validity"/>
    </Description>

    <SupportUrl DefaultValue="https://bouncingsoul182.github.io/outlook-email-validator/support.html"/>

    <Hosts>
        <Host Name="Mailbox"/>
    </Hosts>

    <Requirements>
        <Sets>
            <Set Name="Mailbox" MinVersion="1.1"/>
        </Sets>
    </Requirements>

    <FormSettings>
        <Form xsi:type="ItemRead">
            <FunctionFile DefaultValue="https://bouncingsoul182.github.io/outlook-email-validator/outlook-email-validator.html"/>
        </Form>
    </FormSettings>

    <Runtime>
        <SourceLocation DefaultValue="https://bouncingsoul182.github.io/outlook-email-validator/outlook-email-validator.html"/>
    </Runtime>

    <DefaultSettings>
        <SourceLocation DefaultValue="https://bouncingsoul182.github.io/outlook-email-validator/outlook-email-validator.html"/>
    </DefaultSettings>

    <Resources>
        <Images>
            <Image Id="IconUrl" DefaultValue="https://bouncingsoul182.github.io/outlook-email-validator/icon-64.png"/>
            <Image Id="HighResolutionIconUrl" DefaultValue="https://bouncingsoul182.github.io/outlook-email-validator/icon-128.png"/>
        </Images>
    </Resources>
</OfficeApp>







